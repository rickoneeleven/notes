{
    "name": "notes-app/versioning-system",
    "description": "PHP testing infrastructure for notes application versioning system",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "require": {
        "php": ">=8.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}